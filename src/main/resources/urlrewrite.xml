<?xml version="1.0" encoding="utf-8"?>
 <!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
 
<urlrewrite>
<rule>
    <name>Domain Name Check</name>
    <condition name="host" operator="notequal">www.yourdomain.com</condition>
    <from>^(.*)$</from>
    <to type="redirect">http://www.yourdomain.com$1</to>
</rule>
 </urlrewrite>
